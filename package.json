  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run lint && npm run gen-docs && npm run compile",
    "gen-docs": "typedoc --out docs/ src/ --ignoreCompilerErrors",
    "clean": "rimraf dist",
    "compile": "tsc -b",
    "lint": "tslint --project .",
