    "test": "npm run lint && npm run compile && jest --verbose",
    "test-new": "npm run lint && npm run compile && npm run clear-data && jest -o --verbose",
    "build-and-publish": "npm run build && npm publish"
  },
  "devDependencies": {
    "rimraf": "^3.0.1",
    "ts-node": "^9.1.1",
    "tslint": "^5.20.1",
    "typedoc": "^0.19.2",
    "typescript": "^4.1.3"
  },
